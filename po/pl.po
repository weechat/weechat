msgid ""
"hook: signal, hsignal, modifier, line, print, command, command_run, timer, "
"config, focus, info, info_hashtable"
msgstr ""
"hook: signal, hsignal, modifier, line, print, command, command_run, timer, "
"config, focus, info, info_hashtable"

