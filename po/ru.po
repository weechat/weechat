"    /trigger add resize_big signal signal_sigwinch \"${info:term_width} >= "
"100\" \"\" \"/bar show nicklist\"\n"
"  silently save config each hour:\n"
"    /trigger add cfgsave timer 3600000;0;0 \"\" \"\" \"/mute /save\"\n"
