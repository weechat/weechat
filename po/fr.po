msgstr "envoyer un ping au serveur"

msgid "<server1> [<server2>]"
msgstr "<serveur1> [<serveur2>]"

msgid ""
"server1: server\n"
"server2: forward ping to this server"
msgstr ""
"serveur1: serveur\n"
"serveur2: faire suivre le ping à ce serveur"

