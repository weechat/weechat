msgstr "<nick> [<nick>...]"

msgid "remove channel operator status from nick(s)"
msgstr "revoca lo stato di operatore del canale da uno o più nick"

msgid "remove voice from nick(s)"
msgstr "revoca il voice da uno o più nick"

