msgstr "  %s%s%s: %s%s%s%s%s%s%s"

#, c-format
msgid "  %s%s%s: %s%s%s%s%s%s%s%s%s%s%s%s%s"
msgstr "  %s%s%s: %s%s%s%s%s%s%s%s%s%s%s%s%s"

