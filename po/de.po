msgstr "Auflistung der Trigger:"

#, c-format
msgid "%s%s: invalid hook type: \"%s\""
msgstr "%s%s: ungültiger hook-Typ: \"%s\""

#, c-format
msgid "%s%s: invalid format for regular expression: \"%s\""
msgstr "%s%s: ungültiges Format für regulären Ausdruck: \"%s\""

